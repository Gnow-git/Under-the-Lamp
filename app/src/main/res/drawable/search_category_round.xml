<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Drop Shadow Stack -->
    <item> <shape> <padding android:top="1dp" android:right="2dp" android:bottom="2dp" android:left="1dp" /> <corners android:radius="20dp" /> <solid android:color="#00CCCCCC" /> </shape> </item>
    <item> <shape> <padding android:top="1dp" android:right="2dp" android:bottom="2dp" android:left="1dp" /> <corners android:radius="20dp" /> <solid android:color="#10CCCCCC" /> </shape> </item>
    <item> <shape> <padding android:top="1dp" android:right="2dp" android:bottom="2dp" android:left="1dp" /> <corners android:radius="20dp" /> <solid android:color="#20CCCCCC" /> </shape> </item>
    <item> <shape> <padding android:top="0dp" android:right="2dp" android:bottom="2dp" android:left="0dp" /> <corners android:radius="20dp" /> <solid android:color="#30CCCCCC" /> </shape> </item>
    <item> <shape> <padding android:top="0dp" android:right="2dp" android:bottom="2dp" android:left="0dp" /> <corners android:radius="20dp" /> <solid android:color="#50CCCCCC" /> </shape> </item>
    <!-- Background -->
    <item><shape
        android:padding="10dp"
        android:shape="rectangle">
        <solid android:color="#ffffff" />

        <stroke
            android:width="1dp"
            android:color="#FFFFFF"/>
        <corners
            android:bottomLeftRadius="20dp"
            android:bottomRightRadius="20dp"
            android:topLeftRadius="20dp"
            android:topRightRadius="20dp" />
    </shape></item>
</layer-list>
