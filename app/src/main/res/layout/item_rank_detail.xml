<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rankDetailForm"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:layout_marginBottom="20dp">
    <TextView
        android:id="@+id/rankDetailTitle"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="1.공모전 제목"
        android:textColor="@color/rank_detail_font"
        android:fontFamily="@font/pretendard_regular"
        android:textSize="12sp"
        android:letterSpacing="-0.05"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.0"/>

    <TextView
        android:id="@+id/rankDetailContent"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="공모전 내용"
        android:textColor="@color/rank_detail_font"
        android:fontFamily="@font/pretendard_regular"
        android:textSize="12sp"
        android:letterSpacing="-0.05"
        app:layout_constraintTop_toBottomOf="@+id/rankDetailTitle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>