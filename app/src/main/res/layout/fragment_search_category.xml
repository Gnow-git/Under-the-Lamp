<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/art_view"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_marginEnd="222dp"
            android:layout_marginBottom="140dp"
            android:background="@drawable/search_category_round"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/public_music_view"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:layout_marginBottom="140dp"
            android:background="@drawable/search_category_round"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/music_view"
            app:layout_constraintStart_toEndOf="@+id/art_view"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/music_view"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="140dp"
            android:background="@drawable/search_category_round"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <View
            android:id="@+id/play_view"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_marginEnd="222dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/search_category_round"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/art_view" />

        <View
            android:id="@+id/literature_view"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/search_category_round"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/video_view"
            app:layout_constraintStart_toEndOf="@+id/play_view"
            app:layout_constraintTop_toBottomOf="@id/public_music_view" />

        <View
            android:id="@+id/video_view"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_alignParentStart="true"
            android:layout_marginTop="20dp"
            android:background="@drawable/search_category_round"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/music_view"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/art"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/art_view"
            app:layout_constraintEnd_toEndOf="@+id/art_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/art_view"
            app:layout_constraintTop_toTopOf="@+id/art_view"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/art_un" />

        <TextView
            android:id="@+id/art_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="미술"
            android:fontFamily="@font/pretendard_regular"
            android:textSize="14sp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintBottom_toBottomOf="@+id/art_view"
            app:layout_constraintEnd_toEndOf="@+id/art_view"
            app:layout_constraintStart_toStartOf="@+id/art_view"
            app:layout_constraintTop_toBottomOf="@+id/art" />

        <ImageView
            android:id="@+id/public_music"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/public_music_view"
            app:layout_constraintEnd_toEndOf="@+id/public_music_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/public_music_view"
            app:layout_constraintTop_toTopOf="@+id/music_view"
            app:srcCompat="@drawable/public_music_un" />

        <TextView
            android:id="@+id/public_music_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="대중음악"
            android:fontFamily="@font/pretendard_regular"
            android:textSize="14sp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/public_music_view"
            app:layout_constraintEnd_toEndOf="@+id/public_music_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/public_music_view"
            app:layout_constraintTop_toBottomOf="@+id/public_music" />

        <ImageView
            android:id="@+id/music"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/music_view"
            app:layout_constraintEnd_toEndOf="@+id/music_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/music_view"
            app:layout_constraintTop_toTopOf="@+id/music_view"
            app:srcCompat="@drawable/music_un" />

        <TextView
            android:id="@+id/music_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="음악"
            android:fontFamily="@font/pretendard_regular"
            android:textSize="14sp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/music_view"
            app:layout_constraintEnd_toEndOf="@+id/music_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/music_view"
            app:layout_constraintTop_toBottomOf="@+id/music" />

        <ImageView
            android:id="@+id/play"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/play_view"
            app:layout_constraintEnd_toEndOf="@+id/play_view"
            app:layout_constraintStart_toStartOf="@+id/play_view"
            app:layout_constraintTop_toTopOf="@+id/play_view"
            app:srcCompat="@drawable/play_un" />

        <TextView
            android:id="@+id/play_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="연극"
            android:fontFamily="@font/pretendard_regular"
            android:textSize="14sp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/play_view"
            app:layout_constraintEnd_toEndOf="@+id/play_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/play_view"
            app:layout_constraintTop_toBottomOf="@+id/play" />

        <ImageView
            android:id="@+id/literature"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/literature_view"
            app:layout_constraintEnd_toEndOf="@+id/literature_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/literature_view"
            app:layout_constraintTop_toTopOf="@+id/literature_view"
            app:srcCompat="@drawable/literature_un" />

        <TextView
            android:id="@+id/literature_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="문학"
            android:fontFamily="@font/pretendard_regular"
            android:textSize="14sp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/literature_view"
            app:layout_constraintEnd_toEndOf="@+id/literature_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/literature_view"
            app:layout_constraintTop_toBottomOf="@+id/literature" />

        <ImageView
            android:id="@+id/video"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="25dp"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toBottomOf="@+id/video_view"
            app:layout_constraintEnd_toEndOf="@+id/video_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/video_view"
            app:layout_constraintTop_toTopOf="@+id/video_view"
            app:srcCompat="@drawable/video_un" />

        <TextView
            android:id="@+id/video_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="영상"
            android:fontFamily="@font/pretendard_regular"
            android:textSize="14sp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/video_view"
            app:layout_constraintEnd_toEndOf="@+id/video_view"
            app:layout_constraintHorizontal_bias="0.43"
            app:layout_constraintStart_toStartOf="@+id/video_view"
            app:layout_constraintTop_toBottomOf="@+id/video" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>